              "shrink": 0
            },
            "propConfig": {
              "custom.PartCounter": {
                "binding": {
                  "config": {
                    "aggregate": "MinMax",
                    "avoidScanClassValidation": true,
                    "dateRange": {
                      "endDate": "{../XYChart.custom.EndDateTime}",
                      "startDate": "{../XYChart.custom.StartDateTime}"
                    },
                    "enableValueCache": true,
                    "ignoreBadQuality": false,
                    "preventInterpolation": false,
                    "returnFormat": "Wide",
                    "returnSize": {
                      "numRows": "1",
                      "type": "FIXED"
                    },
                    "tags": "{this.custom.PartCount}",
                    "valueFormat": "DATASET"
                  },
                  "type": "tag-history"
                }
              },
              "props.text": {
                "binding": {
                  "config": {
                    "expression": "concat(round({/root.custom.EnergyPerPartOutput}[0,2],1),\" kWh\")"
