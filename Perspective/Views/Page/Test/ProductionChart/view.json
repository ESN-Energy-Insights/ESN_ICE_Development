          }
        },
        "position": {
          "height": 61,
          "rotate": {
            "anchor": "0% -12%"
          },
          "width": 100,
          "x": 550
        },
        "props": {
          "direction": "column"
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Label_6"
            },
            "position": {
              "basis": "18px"
            },
            "props": {
              "alignVertical": "bottom",
              "text": "Total Energy",
              "textStyle": {
                "classes": "All_Text/Input_Label_Tex_tiny"
              }
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Label_7"
            },
            "props": {
              "alignVertical": "bottom",
              "text": "during prod. hrs",
              "textStyle": {
                "classes": "All_Text/Input_Label_Tex_tiny"
              }
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Label_5"
            },
            "position": {
              "basis": "31px",
              "shrink": 0
            },
            "propConfig": {
              "custom.PartCounter": {
                "binding": {
                  "config": {
                    "avoidScanClassValidation": true,
                    "calculations": [
                      "MinMax"
                    ],
                    "dateRange": {
                      "endDate": "{../XYChart.custom.EndDateTime}",
                      "startDate": "{../XYChart.custom.StartDateTime}"
                    },
                    "enableValueCache": true,
                    "ignoreBadQuality": false,
                    "preventInterpolation": false,
                    "returnFormat": "Calculations",
                    "tags": "{this.custom.Energy2}",
                    "valueFormat": "DATASET"
                  },
                  "type": "tag-history"
                }
              },
              "props.text": {
                "binding": {
                  "config": {
                    "expression": "concat(round({/root.custom.TotalProductionEnergy}[0,2]),\" kWh\")"
                  },
                  "type": "expr"
                }
              }
            },
            "props": {
              "textStyle": {
                "classes": "All_Text/Input_Label_Text_small",
                "textAlign": "center"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "FlexContainer",
          "tooltip": {
            "enabled": true,
            "text": "Total energy consumed during the production hours.  This includes energy consumed when parts were being made, but also any energy consumed that was below the part-count power threshold."
          }
        },
        "position": {
          "height": 61,
          "rotate": {
            "anchor": "0% -12%"
          },
          "width": 90,
          "x": 657,
          "y": 1
        },
        "props": {
          "direction": "column"
        },
        "type": "ia.container.flex"
      },
      {
        "meta": {
          "name": "Label_0"
        },
        "position": {
          "height": 32,
          "width": 239,
          "x": 80,
          "y": 138
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "parent.custom.StartDateTime"
              },
              "type": "property"
            }
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Label_1"
        },
        "position": {
          "height": 32,
          "width": 294,
          "x": 326,
          "y": 138
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "parent.custom.EndDateTime"
              },
              "type": "property"
            }
          }
        },
        "type": "ia.display.label"
      }
    ],
    "custom": {
      "EnergyPerPart": [
        {
          "aggregate": "SimpleAverage",
          "alias": "Energy"
        }
      ],
      "EnergyRange": [
        {
          "aggregate": "LastValue",
          "alias": "Energy",
          "path": "[ESN]Opto22/EdgeNodes/EMK1-0001/Loads/Compressor1/Data/Totals/Energy/Total_PositiveEnergy"
        }
      ],
      "OnDuration": [
        {
          "aggregate": "DurationOn",
          "alias": "Ontime",
          "path": "[ESN]Opto22/EdgeNodes/EMK1-0001/Loads/Compressor1/DashboardSettings/Production/ProductionBooleanSimple"
        }
      ],
      "OnOff": [
        {
          "aggregate": "LastValue",
          "alias": "Status",
          "path": "[ESN]Opto22/EdgeNodes/EMK1-0001/Loads/Compressor1/DashboardSettings/Production/ProductionBooleanSimple"
        }
      ],
      "PartCount": [
        {
          "aggregate": "CountOn",
          "alias": "ProdCount",
          "path": "[ESN]Opto22/EdgeNodes/EMK1-0001/Loads/Compressor1/DashboardSettings/Production/Part Boolean"
        }
      ],
      "ProductionStartEnergy": "value",
      "key": "value"
    },
    "meta": {
      "contextMenu": {
        "style": {
          "backgroundColor": "#AAAAAA"
        }
      },
      "name": "root"
    },
    "position": {
      "x": -61,
      "y": 11
    },
    "propConfig": {
      "custom.Date": {
        "binding": {
          "config": {
            "expression": "midnight({view.params.Date})"
          },
          "type": "expr"
        }
      },
      "custom.EndDateTime": {
        "binding": {
          "config": {
            "expression": "addMinutes(addHours({this.custom.Date},getHour24({this.custom.ProductionEndTime})),getMinute({this.custom.ProductionEndTime}))"
          },
          "type": "expr"
        }
      },
      "custom.EnergyPerPartOutput": {
        "binding": {
          "config": {
            "avoidScanClassValidation": true,
            "calculations": [
              "SimpleAverage"
            ],
            "dateRange": {
              "endDate": "{this.custom.EndDateTime}",
              "startDate": "{this.custom.StartDateTime}"
            },
            "enableValueCache": true,
            "ignoreBadQuality": false,
            "preventInterpolation": false,
            "returnFormat": "Calculations",
            "tags": "{this.custom.EnergyPerPart}",
            "valueFormat": "DATASET"
          },
          "type": "tag-history"
        }
      },
      "custom.EnergyPerPart[0].path": {
        "binding": {
          "config": {
            "expression": "{view.params.tagpath}+\"/DashboardSettings/Production/Part Energy\""
          },
          "type": "expr"
        }
      },
      "custom.PartCountOutput": {
        "binding": {
          "config": {
            "aggregate": "MinMax",
            "avoidScanClassValidation": true,
            "dateRange": {
              "endDate": "{this.custom.EndDateTime}",
              "startDate": "{this.custom.StartDateTime}"
            },
            "enableValueCache": true,
            "ignoreBadQuality": false,
            "preventInterpolation": false,
            "returnFormat": "Wide",
            "returnSize": {
              "numRows": "1",
              "type": "FIXED"
            },
            "tags": "{this.custom.PartCount}",
            "valueFormat": "DATASET"
          },
          "type": "tag-history"
        }
      },
      "custom.ProducingPercent": {
        "binding": {
          "config": {
            "aggregate": "MinMax",
            "avoidScanClassValidation": true,
            "dateRange": {
              "endDate": "getDate(2024,5,6)",
              "startDate": "getDate(2024,5,5)"
            },
            "enableValueCache": true,
            "ignoreBadQuality": false,
            "preventInterpolation": false,
            "returnFormat": "Wide",
            "returnSize": {
              "numRows": "100",
              "type": "FIXED"
            },
            "tags": "{this.custom.OnDuration}",
            "valueFormat": "DATASET"
          },
          "transforms": [
            {
              "expression": "{value}/{this.custom.Seconds}\r\n//{value}[0,2]/{this.custom.Seconds}",
              "type": "expression"
            }
          ],
          "type": "tag-history"
        }
      },
      "custom.ProductionEndEnergy": {
        "binding": {
          "config": {
            "aggregate": "MinMax",
            "avoidScanClassValidation": true,
            "dateRange": {
              "endDate": "{this.custom.ProductionStartTime}",
              "startDate": "addMinutes({this.custom.ProductionStartTime},-1)"
            },
            "enableValueCache": true,
            "ignoreBadQuality": false,
            "preventInterpolation": false,
            "returnFormat": "Wide",
            "returnSize": {
              "numRows": "1",
              "type": "FIXED"
            },
            "tags": "{this.custom.EnergyRange}",
            "valueFormat": "DATASET"
          },
          "type": "tag-history"
        }
      },
      "custom.ProductionEndTime": {
        "binding": {
          "config": {
            "fallbackDelay": 2.5,
            "mode": "indirect",
            "references": {
              "tagpath": "{view.params.tagpath}"
            },
            "tagPath": "{tagpath}/DashboardSettings/Production/ProductionEndDatetime"
          },
          "type": "tag"
        }
      },
      "custom.ProductionStartTime": {
        "binding": {
          "config": {
            "fallbackDelay": 2.5,
            "mode": "indirect",
            "references": {
              "tagpath": "{view.params.tagpath}"
            },
            "tagPath": "{tagpath}/DashboardSettings/Production/ProductionStartDatetime"
          },
          "type": "tag"
        }
      },
      "custom.Seconds": {
        "binding": {
          "config": {
            "expression": "secondsBetween({this.custom.StartDateTime},{this.custom.EndDateTime})"
          },
          "type": "expr"
        }
      },
      "custom.StartDateTime": {
        "binding": {
          "config": {
            "expression": "addMinutes(addHours({this.custom.Date},getHour24({this.custom.ProductionStartTime})),getMinute({this.custom.ProductionStartTime}))"
          },
          "type": "expr"
        }
      },
      "custom.TotalProductionEnergy": {
        "binding": {
          "config": {
            "avoidScanClassValidation": true,
            "calculations": [
              "Range"
            ],
            "dateRange": {
              "endDate": "{this.custom.EndDateTime}",
              "startDate": "{this.custom.StartDateTime}"
            },
            "enableValueCache": true,
            "ignoreBadQuality": false,
            "preventInterpolation": false,
            "returnFormat": "Calculations",
            "tags": "{this.custom.EnergyRange}",
            "valueFormat": "DATASET"
          },
          "type": "tag-history"
        }
      }
    },
    "props": {
      "style": {
        "backgroundColor": "#ECEFF6",
        "margin": 0,
        "overflow": "hidden",
        "overflowX": "hidden",
        "overflowY": "hidden",
        "padding": 0
      }
    },
    "type": "ia.container.coord"
  }
}