        "meta": {
          "name": "TabContainer"
        },
        "position": {
          "basis": "300px",
          "grow": 1
        },
        "propConfig": {
          "props.tabs": {
            "binding": {
              "config": {
                "path": "view.custom.deviceType"
              },
              "transforms": [
                {
                  "code": "\t#if self.session.custom.companyID \u003d\u003d \u0027Opto22\u0027:\n\t\t\n\tif value \u003d\u003d \"EMU\":\n\t\treturn [\"Overview\", \"Energy\", \"Power\", \"Power Quality\", \"Production\",\"Startup Power\", \"Monthly\"]\n\telse:\n\t\treturn [\"Overview\"]",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "menuType": "modern",
          "tabSize": {
            "height": 45,
            "width": 95
          },
          "tabStyle": {
            "active": {
              "fontSize": 16,
              "whiteSpace": "pre-line"
            },
            "inactive": {
              "fontSize": 16,
              "whiteSpace": "pre-line"
            }
          }
        },
        "type": "ia.container.tab"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Page/Page"
      }
    },
    "type": "ia.container.flex"
  }
